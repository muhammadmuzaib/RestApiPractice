{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["password", "firstName", "lastName"],
  "properties": {
    "password": {
      "type": "string",
      "minLength": 8
    },
    "firstName": {
      "type": "string",
      "minLength": 1
    },
    "lastName": {
      "type": "string",
      "minLength": 1
    }
  }
}